package main

import "fmt"

func main() {
    array := [2][3]int{{1, 2, 3}, {4, 5, 6}}

    var linha, coluna int
    fmt.Print("Informe o índice da linha: ")
    fmt.Scan(&linha)
    fmt.Print("Informe o índice da coluna: ")
    fmt.Scan(&coluna)

    valor := array[linha][coluna]

    fmt.Printf("Valor na posição [%d][%d]: %d\n", linha, coluna, valor)
}
